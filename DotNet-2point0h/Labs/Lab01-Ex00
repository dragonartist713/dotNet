string[] words = new string[3];

for(int i = 0; i < words.length; i++){
    System.out.println(words[i].length());
};




int[] myArray = new int[1001];

for(int i = 0; i < myArray.length; i++){
    if(i % 0){
        System.out.println("Even!");
    };
    if(i % 1){
        System.out.println("Odd!");
    };
};